//////////////////////////////////
// OpenFPS TPC Plugin Tester
// Testing basic efficiency
//////////////////////////////////
sync on
sync rate 0
disable escapekey

start:
cls 0
set cursor 0,0
`print "Press any key to test algorithms..."
`wait key
print "Testing:"

//test values

// Testing a distance algorithm

//dbpro native
starttime = hitimer(10000000)
//perform 10000 distance calculations
for n = 0 to 10000:
    //arbitrary values
    x#=0-n
    y#=0.0
    z#=n
    x2#=n
    y2#=50.0
    z2#=0-n
    value_calculation#=GetDistance( x# , y# , z#, x2# , y2# , z2# )
next n

endtime = hitimer(10000000)
elapsedtime# = (endtime-starttime)/10000.0
print "DBPRO NATIVE: ";elapsedtime#;" ms"

//OpenFPS plugin
starttime = hitimer(10000000)
//perform 10000 distance calculations
for n = 0 to 10000:
    //arbitrary values
    x#=0-n
    y#=0.0
    z#=n
    x2#=n
    y2#=50.0
    z2#=0-n
    value_calculation#=OpenFPS_DistanceFormula( x# , y# , z#, x2# , y2# , z2# )
next n
endtime = hitimer(10000000)
elapsedtime# = (endtime-starttime)/10000.0
print "OPENFPS PLUGIN: ";elapsedtime#;" ms"

`print "Press any key to go to the start..."
sync
if escapekey() then end
`wait key
goto start

Function GetDistance( obj1X# , obj1Y# , obj1Z#, obj2X# , obj2Y# , obj2Z# )
xx# = obj1X# - obj2X#
yy# = obj1Y# - obj2Y#
zz# = obj1Z# - obj2Z#
thisDistance2# = Sqrt((xx#*xx#)+(yy#*yy#)+(zz#*zz#))
Endfunction thisDistance2#